{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.10" }
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/app/(.*)", "dest": "/frontend/$1" },
    { "src": "/(.*)", "dest": "main.py" }
  ]
}
