{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@3.12",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "env": {
    "PYTHON_VERSION": "3.9",
    "PIP_DISABLE_PIP_VERSION_CHECK": "1"
  }
}
