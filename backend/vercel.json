{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "maxDuration": 30,
      "memory": 1024,
      "excludeFiles": "{tests/**,__tests__/**,**/*.test.py,**/test_*.py,fixtures/**,__fixtures__/**,testdata/**,sample-data/**,static/**,assets/**}"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "env": {
    "PIP_DISABLE_PIP_VERSION_CHECK": "1"
  }
}
